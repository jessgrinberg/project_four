<h1>New Map</h1>

<body onmousemove = "initialize()">

<%= form_for(@map) do |f| %>
  <% if @map.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@map.errors.count, "error") %> prohibited this map from being saved:</h2>

      <ul>
      <% @map.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>




  <%= form_tag( :method => "get", id: "search-form") do %>

 <div class="field">
    <%= f.label :organization %><br>
    <%= f.text_field :organization %>
  </div>


   <div class="field">	
    <%= text_field_tag "map[address]", nil, placeholder: 'Enter your address', :id => 'autocomplete', :onFocus => 'geolocate()' %>
      </div>


 <div class="actions">
    <%= submit_tag "Search", id: "search-submit", value: "Submit" %>
    </div> 
 <% end %>

<!--     <%= f.submit %> -->
<% end %>



<%= render 'display'%>




</body>
